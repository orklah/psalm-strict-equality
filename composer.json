{
    "name": "orklah/psalm-strict-equality",
    "description": "Automatically change == into === when safe ",
    "type": "psalm-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "orklah"
        }
    ],
    "extra": {
        "psalm" : {
            "pluginClass": "Orklah\\StrictEquality\\Plugin"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "vimeo/psalm": "^4.6.1|^5.0",
        "nikic/php-parser": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Orklah\\StrictEquality\\": ["./src"],
            "Orklah\\StrictEquality\\Tests\\": ["./tests"]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15.0"
    }
}
